@action('edit_post', method=["GET", "POST"])
@action.uses(db, auth.user, session, 'editpost.html')
def edit_post():
    pid = request.params.get('id')
    p = db.posts[pid]
    print(p)
    form = Form(db.posts, record=p, csrf_session=session, formstyle=FormStyleBulma)
    print("After form")
    if form.accepted:
        redirect(URL('profile'))
    return dict(form=form)


